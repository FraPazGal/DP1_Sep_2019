	<bean id="CONFERENCE" class="domain.Category">
		<property name="name">
			<util:map id="utilmap" map-class="java.util.HashMap"
				key-type="java.lang.String" value-type="java.lang.String">
				<entry key="Español" value="CONFERENCIA" />
